<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="UrbanAirship.InAppHtml"/>

    <style name="UrbanAirship.InAppHtml.Layout"/>

    <style name="UrbanAirship.InAppHtml.DismissButton">
        <item name="android:background">@null</item>
        <item name="android:src">@drawable/ua_ic_close_white_18dp</item>
        <item name="android:layout_marginLeft">4dp</item>
        <item name="android:layout_marginRight">4dp</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:contentDescription">@string/ua_cancel</item>
        <item name="android:layout_gravity">top|end</item>
    </style>

    <style name="UrbanAirship.InAppHtml.WebView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="UrbanAirship.InAppHtml.Progress">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
    </style>

</resources>
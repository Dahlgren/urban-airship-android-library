<resources>
    <!-- The corner radius for in-app message banner views -->
    <dimen name="ua_iam_banner_corner_radius">4dp</dimen>

    <!-- The elevation for in-app message banners. Only applicable for card views or API 21 or newer -->
    <dimen name="ua_iam_banner_elevation">4dp</dimen>

    <!-- The in-app message banner width -->
    <dimen name="ua_iam_banner_width">320dp</dimen>

    <!-- Max in-app message banner width -1 = match_parent -->
    <dimen name="ua_iam_banner_max_width">-1px</dimen>
</resources>